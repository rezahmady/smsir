{
    "name": "rezahmady/smsir",
    "description": "",
    "keywords": [
        "laravel",
        "sms",
        "پیامک",
        "اس ام اس",
        "sendsms",
        "notifacation",
        "sms.ir",
        "smsir"
    ],
    "homepage": "https://github.com/rezahmady/smsir",
    "license": "MIT",
    "authors": [{
        "name": "Reza Ahmadi Sabzevar",
        "email": "ahmadireza15@gmail.com",
        "homepage": "https://sityno.com",
        "role": "Developer"
    }],
    "require": {
        "php": "~7.3",
        "ext-json": "*",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rezahmady\\Smsir\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rezahmady\\Smsir\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rezahmady\\Smsir\\SmsirServiceProvider"
            ],
            "aliases": {
                "SMSIR": "Rezahmady\\Smsir\\Facades\\Smsir"
            }
        }
    }
}